from netmiko import ConnectHandler

csr1kv = {
    "device_type" : "cisco_ios",
    "ip" : "192.168.56.101",
    "username" : "cisco",
    "password" : "cisco123!",
}

session = ConnectHandler(**csr1kv)
session.enable
command = session.send_command('show ip int brief')
print(command)
session.exit_enable_mode
